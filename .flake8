[flake8]
max-line-length = 120
ignore = W605
per-file-ignores =
    **/tests.py:E501
exclude =
    */migrations/

[coverage:report]
omit =
    */migrations/*.py
    */settings.py
    */apps.py
    */wsgi.py
    */tests.py
    */__init__.py
    */urls.py
    */manage.py
    */request_handler.py
    */medexCms/models.py
    */medexCms/test/mocks.py
